@model IEnumerable<SubscriptionTrackerApp.Models.ServiceType.ServiceTypeListItem>

@* @{
ViewData["Title"] = "ServiceType Index";
} *@

<h1>Service Types page</h1>
<a asp-action="Create">"Create new Service Type</a>

@if (Model.Count() > 0)
{
    <div>
        @foreach (var service in Model)
        {
            <div>
                <h4>@service.Name</h4>
                <div>
                    <p>Description: @service.Description</p>
                    <p>Priority: @service.Priority</p>
                </div>
            </div>
        }
    </div>
}


else
{
    <p>There are no results for this page.</p>
}